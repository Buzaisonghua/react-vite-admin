{
  "name": "vite-project",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "prepare": "husky",
    "lint": "eslint . --cache",
    "lint:fix": "eslint . --fix",
    "lint:prettier": "prettier --write --loglevel warn \"src/**/*.{js,ts,json,tsx,css,less,scss,html,md}\"",
    "lint:style": "stylelint **/*.{less,postcss,css,scss} --cache",
    "lint:styleFix": "lint:style --fix",
    "cz": "git pull && git add -A && git-cz",
    "log": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0 && git add CHANGELOG.md"
  },
  "dependencies": {
    "@ant-design/icons": "^6.0.0",
    "@reduxjs/toolkit": "^2.6.1",
    "antd": "^5.23.2",
    "axios": "^1.8.3",
    "js-cookie": "^3.0.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-redux": "^9.2.0",
    "react-router": "^7.1.3",
    "react-router-dom": "^7.1.3"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.15.0",
    "@commitlint/cli": "^19.6.1",
    "@commitlint/config-conventional": "^19.6.0",
    "@eslint-react/eslint-plugin": "^1.24.1",
    "@types/js-cookie": "^3.0.6",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "commitizen": "^4.3.1",
    "conventional-changelog-cli": "^5.0.0",
    "cz-git": "^1.11.0",
    "eslint": "^9.18.0",
    "eslint-plugin-format": "^1.0.1",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.18",
    "husky": "^9.1.7",
    "less": "^4.2.2",
    "lint-staged": "^15.4.1",
    "mockjs": "^1.1.0",
    "prettier": "^3.4.2",
    "stylelint": "^14.9.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recess-order": "^3.0.0",
    "stylelint-config-standard": "^26.0.0",
    "stylelint-less": "^1.0.6",
    "typescript": "~5.6.2",
    "unocss": "^65.4.3",
    "vite": "^6.0.5"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  }
}
